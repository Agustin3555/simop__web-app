{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.cache/tsconfig.node.tsbuildinfo",
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "CommonJS",
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "types": ["node"],
    "noEmit": true,
    "strict": true
  },
  "include": ["vite.config.ts"],
  "exclude": ["node_modules"]
}
